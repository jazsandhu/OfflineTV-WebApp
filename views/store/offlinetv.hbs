<div class="container">
	<div class="row">
		<div class="col-md-12 text-center mt-4">
			<p>Offline TV</p>
		</div>
	</div>
	<div class="row">
		{{#each data}}
		<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 my-2">
			<div class="product">
				<a href="https://teespring.com{{url}}">
					<div class="card">
						<img class="card-img-top" src="{{image_url}}" alt="{{name}}">
						<div class="card-body">
							<h5 class="card-title">{{name}}</h5>
							<p class="card-text">{{product_name}}</p>
							<p class="card-text">{{price}}</p>
						</div>
					</div>
				</a>
			</div>
		</div>
		{{/each}}
	</div>
</div>