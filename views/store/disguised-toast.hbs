<div class="container">
	<div class="row">
		<div class="col-md-12 text-center mt-4">
			<p>Disguised Toast</p>
		</div>
	</div>
	<div class="row mt-4">
		<p>
			<i>Words of Wisdom</i>
		</p>
	</div>
	<div class="row">
		{{#each wow}}
		<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 my-2">
			<div class="product">
				<a href="{{productURL}}">
					<div class="card">
						<img class="card-img-top" src="{{imageURL}}" alt="{{ProductName}}">
						<div class="card-body">
							<h5 class="card-title">{{ProductName}}</h5>
							<p class="card-text">{{Price}}</p>
						</div>
					</div>
				</a>
			</div>
		</div>
		{{/each}}
	</div>
	<div class="row mt-4">
		<p>
			<i>Hearthstone Toast</i>
		</p>
	</div>
	<div class="row">
		{{#each ht}}
		<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 my-2">
			<div class="product">
				<a href="{{productURL}}">
					<div class="card">
						<img class="card-img-top" src="{{imageURL}}" alt="{{ProductName}}">
						<div class="card-body">
							<h5 class="card-title">{{ProductName}}</h5>
							<p class="card-text">{{Price}}</p>
						</div>
					</div>
				</a>
			</div>
		</div>
		{{/each}}
	</div>
	<div class="row mt-4">
		<p>
			<i>Just Toast</i>
		</p>
	</div>
	<div class="row">
		{{#each toast}}
		<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 my-2">
			<div class="product">
				<a href="{{productURL}}">
					<div class="card">
						<img class="card-img-top" src="{{imageURL}}" alt="{{ProductName}}">
						<div class="card-body">
							<h5 class="card-title">{{ProductName}}</h5>
							<p class="card-text">{{Price}}</p>
						</div>
					</div>
				</a>
			</div>
		</div>
		{{/each}}
	</div>
</div>