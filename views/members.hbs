<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <h2 style="text-align: center; text-decoration: underline;">The OfflineTV Crew</h2>
    </div>
</div>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        {{#each data}}
            <div style="padding-bottom:4em;" class="col-md-6">
                <div style="float: left">
                    <table width="100%">
                        <tr height="40%">
                            <td rowspan="2" width="30%" >
                                <img class="profilePicture" style="border-radius:100%;" width="100%" height="auto" src="{{profilePicture}}" alt="Frame" />
                            </td>
                            <td colspan="9">
                                <h2 style="color:rgb(73, 73, 73);">&nbsp;{{memberName}}</h3><hr style="margin-bottom:0;padding-bottom:0;">
                            </td>
                        </tr>
                        <tr height="60%" >
                            <td width="10%"> 
                                <div class="SocialMediaLinks"><a href="{{twitchUrl}}"><img width="100%" src="https://i.imgur.com/NXom2um.png" alt="TwitchTV" /></a></div>
                            </td>
                            <td></td>
                            <td width="10%">
                                <div class="SocialMediaLinks"><a href="{{youtubeUrl}}"><img width="100%" src="https://i.imgur.com/Ri73JhH.png" alt="Youtube" /></a></div>
                            </td> 
                            <td></td>
                            <td width="10%">
                                <div class="SocialMediaLinks"><a href="{{instagramUrl}}"><img width="100%" src="https://i.imgur.com/9PHyms0.png" alt="Instagram" /></a></div>
                            </td>
                            <td></td>
                            <td width="10%">
                                <div class="SocialMediaLinks"><a href="{{twitterUrl}}"><img width="100%" src="https://i.imgur.com/Amwt37w.png" alt="Twitter" /></a></div>
                            </td>
                            <td></td>
                            <td width="10%">
                                <div class="SocialMediaLinks"><a href="{{facebookUrl}}"><img width="100%" src="https://i.imgur.com/ZvtUG96.png" alt="Facebook" /></a></div>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        {{/each}}
    </div>
</div>