<div class="row">
    <div class="col-md-12">
        <h2 style="text-align: center; text-decoration: underline;">The OfflineTV Crew</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        {{#each data}}
            <div style="padding: 20px;" class="col-md-6">
                <img class="profilePicture" width="30%" height="30%" src="{{profilePicture}}" alt="Frame" />
                <div style="float: left">
                    <h3 style="margin-left: px;">&nbsp;&nbsp;&nbsp;&nbsp;{{memberName}}</h3><hr>
                    <table style="width:100%">
                        <tr>
                            <td>
                                <a href="{{twitchUrl}}"><img width="30px" src="https://i.imgur.com/NXom2um.png" alt="TwitchTV" /></a>
                            </td>
                            <td>
                                <a href="{{youtubeUrl}}"><img width="30px" src="https://i.imgur.com/Ri73JhH.png" alt="Youtube" /></a>
                            </td>
                            <td>
                                <a href="{{instagramUrl}}"><img width="30px" src="https://i.imgur.com/9PHyms0.png" alt="Instagram" /></a>
                            </td>
                            <td>
                                <a href="{{twitterUrl}}"><img width="30px" src="https://i.imgur.com/Amwt37w.png" alt="Twitter" /></a>
                            </td>
                            <td>
                                <a href="{{facebookUrl}}"><img width="30px" src="https://i.imgur.com/ZvtUG96.png" alt="Facebook" /></a>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        {{/each}}
    </div>
</div>